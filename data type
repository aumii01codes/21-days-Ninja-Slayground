import java.util.*;
public class Solution{
    public static int dataTypes(String type){
        if (type.equals("Long")) return 8;
        else if (type.equals("Double")) return 8;
        else if (type.equals("Integer")) return 4;
        else if (type.equals("Float")) return 4;
        else return 1;
    }
  
    public static void main(String[] args) {
        Scanner read= new Scanner(System.in);
        String t= read.nextLine();
        read.close();
        int size= dataTypes(t);
        System.out.println(size);
    }
}
